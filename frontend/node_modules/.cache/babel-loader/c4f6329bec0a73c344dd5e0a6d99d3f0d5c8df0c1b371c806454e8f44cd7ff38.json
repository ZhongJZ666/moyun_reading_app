{"ast":null,"code":"export default {\n  name: 'StudentDashboard',\n  data() {\n    return {\n      mentor: null,\n      activities: [],\n      todos: [],\n      checkedTodos: []\n    };\n  },\n  async created() {\n    // TODO: 从API获取数据\n    this.mentor = {\n      username: 'mentor1',\n      email: 'mentor1@example.com'\n    };\n    this.activities = [{\n      id: 1,\n      content: '提交了读书报告',\n      time: '2024-05-20 10:00',\n      type: 'primary'\n    }, {\n      id: 2,\n      content: '参加了读书会活动',\n      time: '2024-05-19 15:30',\n      type: 'success'\n    }];\n    this.todos = [{\n      id: 1,\n      content: '完成本周读书报告'\n    }, {\n      id: 2,\n      content: '准备下周读书会发言'\n    }];\n  }\n};","map":{"version":3,"names":["name","data","mentor","activities","todos","checkedTodos","created","username","email","id","content","time","type"],"sources":["E:\\moyun-reading-platform\\moyun-reading-platform\\frontend\\src\\views\\student\\StudentDashboard.vue"],"sourcesContent":["<template>\r\n  <div class=\"student-dashboard\">\r\n    <h1>学生工作台</h1>\r\n    <el-row :gutter=\"20\">\r\n      <el-col :span=\"8\">\r\n        <el-card>\r\n          <template #header>\r\n            <div class=\"card-header\">\r\n              <span>我的导师</span>\r\n            </div>\r\n          </template>\r\n          <div class=\"mentor-info\">\r\n            <el-descriptions :column=\"1\" border>\r\n              <el-descriptions-item label=\"用户名\">\r\n                {{ mentor?.username || '暂无导师' }}\r\n              </el-descriptions-item>\r\n              <el-descriptions-item label=\"邮箱\">\r\n                {{ mentor?.email || '-' }}\r\n              </el-descriptions-item>\r\n            </el-descriptions>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n      <el-col :span=\"8\">\r\n        <el-card>\r\n          <template #header>\r\n            <div class=\"card-header\">\r\n              <span>最近活动</span>\r\n            </div>\r\n          </template>\r\n          <div class=\"activity-list\">\r\n            <el-timeline>\r\n              <el-timeline-item\r\n                v-for=\"activity in activities\"\r\n                :key=\"activity.id\"\r\n                :timestamp=\"activity.time\"\r\n                :type=\"activity.type\"\r\n              >\r\n                {{ activity.content }}\r\n              </el-timeline-item>\r\n            </el-timeline>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n      <el-col :span=\"8\">\r\n        <el-card>\r\n          <template #header>\r\n            <div class=\"card-header\">\r\n              <span>待办事项</span>\r\n            </div>\r\n          </template>\r\n          <div class=\"todo-list\">\r\n            <el-empty v-if=\"!todos.length\" description=\"暂无待办\" />\r\n            <el-checkbox-group v-else v-model=\"checkedTodos\">\r\n              <el-checkbox\r\n                v-for=\"todo in todos\"\r\n                :key=\"todo.id\"\r\n                :label=\"todo.id\"\r\n              >\r\n                {{ todo.content }}\r\n              </el-checkbox>\r\n            </el-checkbox-group>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'StudentDashboard',\r\n  data() {\r\n    return {\r\n      mentor: null,\r\n      activities: [],\r\n      todos: [],\r\n      checkedTodos: []\r\n    }\r\n  },\r\n  async created() {\r\n    // TODO: 从API获取数据\r\n    this.mentor = {\r\n      username: 'mentor1',\r\n      email: 'mentor1@example.com'\r\n    }\r\n    this.activities = [\r\n      { id: 1, content: '提交了读书报告', time: '2024-05-20 10:00', type: 'primary' },\r\n      { id: 2, content: '参加了读书会活动', time: '2024-05-19 15:30', type: 'success' }\r\n    ]\r\n    this.todos = [\r\n      { id: 1, content: '完成本周读书报告' },\r\n      { id: 2, content: '准备下周读书会发言' }\r\n    ]\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.student-dashboard {\r\n  padding: 20px;\r\n}\r\n\r\n.card-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.mentor-info,\r\n.activity-list,\r\n.todo-list {\r\n  min-height: 200px;\r\n}\r\n\r\n.el-timeline {\r\n  padding: 0;\r\n  margin: 0;\r\n}\r\n</style> "],"mappings":"AAsEA,eAAe;EACbA,IAAI,EAAE,kBAAkB;EACxBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,IAAI;MACZC,UAAU,EAAE,EAAE;MACdC,KAAK,EAAE,EAAE;MACTC,YAAY,EAAE;IAChB;EACF,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd;IACA,IAAI,CAACJ,MAAK,GAAI;MACZK,QAAQ,EAAE,SAAS;MACnBC,KAAK,EAAE;IACT;IACA,IAAI,CAACL,UAAS,GAAI,CAChB;MAAEM,EAAE,EAAE,CAAC;MAAEC,OAAO,EAAE,SAAS;MAAEC,IAAI,EAAE,kBAAkB;MAAEC,IAAI,EAAE;IAAU,CAAC,EACxE;MAAEH,EAAE,EAAE,CAAC;MAAEC,OAAO,EAAE,UAAU;MAAEC,IAAI,EAAE,kBAAkB;MAAEC,IAAI,EAAE;IAAU,EAC1E;IACA,IAAI,CAACR,KAAI,GAAI,CACX;MAAEK,EAAE,EAAE,CAAC;MAAEC,OAAO,EAAE;IAAW,CAAC,EAC9B;MAAED,EAAE,EAAE,CAAC;MAAEC,OAAO,EAAE;IAAY,EAChC;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}