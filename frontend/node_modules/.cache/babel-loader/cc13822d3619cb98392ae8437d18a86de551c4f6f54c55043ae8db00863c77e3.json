ss=\"quick-actions\">\r\n      <el-button-group>\r\n        <el-button \r\n          type=\"primary\" \r\n          icon=\"el-icon-user\"\r\n          @click=\"$router.push('/admin/users')\"\r\n        >\r\n          用户管理\r\n        </el-button>\r\n        <el-button \r\n          type=\"success\" \r\n          icon=\"el-icon-notebook-2\"\r\n          @click=\"$router.push('/admin/books')\"\r\n        >\r\n          书籍管理\r\n        </el-button>\r\n        <el-button \r\n          type=\"warning\" \r\n          icon=\"el-icon-document-checked\"\r\n          @click=\"$router.push('/admin/posts')\"\r\n        >\r\n          内容审核\r\n        </el-button>\r\n      </el-button-group>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport StatCard from '@/components/admin/StatCard.vue'\r\nimport RecentUsers from '@/components/admin/RecentUsers.vue'\r\nimport RecentReports from '@/components/admin/RecentReports.vue'\r\n\r\nexport default {\r\n  name: 'AdminDashboard',\r\n  components: {\r\n    StatCard,\r\n    RecentUsers,\r\n    RecentReports\r\n  },\r\n  data() {\r\n    return {\r\n      stats: [\r\n        {\r\n          title: '总用户数',\r\n          value: '1,248',\r\n          icon: 'el-icon-user-solid',\r\n          trend: 'up'\r\n        },\r\n        {\r\n          title: '新增书籍',\r\n          value: '56',\r\n          icon: 'el-icon-notebook-2',\r\n          trend: 'up'\r\n        },\r\n        {\r\n          title: '待审内容',\r\n          value: '23',\r\n          icon: 'el-icon-warning',\r\n          trend: 'down'\r\n        },\r\n        {\r\n          title: '举报记录',\r\n          value: '12',\r\n          icon: 'el-icon-flag',\r\n          trend: 'same'\r\n        }\r\n      ]\r\n    }\r\n  },\r\n  async created() {\r\n    await this.$store.dispatch('admin/fetchDashboardData')\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.admin-dashboard {\r\n  padding: 20px;\r\n}\r\n\r\n.stats-row {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.content-row {\r\n  margin: