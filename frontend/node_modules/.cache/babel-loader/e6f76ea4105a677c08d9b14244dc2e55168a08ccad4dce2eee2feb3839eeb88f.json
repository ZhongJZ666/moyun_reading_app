 class=\"recent-books\">\r\n        <template #header>\r\n          <div class=\"card-header\">\r\n            <span>最近阅读</span>\r\n            <el-button \r\n              type=\"text\"\r\n              @click=\"$router.push('/books')\"\r\n            >\r\n              查看更多\r\n            </el-button>\r\n          </div>\r\n        </template>\r\n        <book-carousel :books=\"recentBooks\" />\r\n      </el-card>\r\n      \r\n      <!-- 待办事项 -->\r\n      <el-row :gutter=\"20\" class=\"todo-row\">\r\n        <el-col :span=\"12\">\r\n          <todo-list />\r\n        </el-col>\r\n        <el-col :span=\"12\">\r\n          <calendar-widget />\r\n        </el-col>\r\n      </el-row>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import { mapState } from 'vuex'\r\n  import StatCard from '@/components/home/StatCard.vue'\r\n  import BookCarousel from '@/components/book/BookCarousel.vue'\r\n  import TodoList from '@/components/home/TodoList.vue'\r\n  import CalendarWidget from '@/components/home/CalendarWidget.vue'\r\n  \r\n  export default {\r\n    name: 'DashboardView',\r\n    components: {\r\n      StatCard,\r\n      BookCarousel,\r\n      TodoList,\r\n      CalendarWidget\r\n    },\r\n    data() {\r\n      return {\r\n        stats: [\r\n          {\r\n            title: '本周阅读',\r\n            value: '5本',\r\n            icon: 'el-icon-notebook-2',\r\n            color: '#409EFF'\r\n          },\r\n          {\r\n            title: '读书笔记',\r\n            value: '12篇',\r\n            icon: 'el-icon-edit',\r\n            color: '#67C23A'\r\n          },\r\n          {\r\n            title: '圈子动态',\r\n            value: '23条',\r\n            icon: 'el-icon-chat-line-round',\r\n            color: '#E6A23C'\r\n          },\r\n          {\r\n            title: '待办事项',\r\n            value: '3项',\r\n            icon: 'el-icon-alarm-clock',\r\n            color: '#F56C6C'\r\n          }\r\n        ]\r\n      }\r\n    },\r\n    computed: {\r\n      ...mapState('books', ['recentBooks'])\r\n    },\r\n    async created() {\r\n      await this.$store.dispatch('books/fetchRecentBooks')\r\n    }\r\n  }\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .dashboard-container {\r\n    padding: 20px;\r\n    max-width: 1200px;\r\n    margin: 0 auto;\r\n  }\r\n  \r\n  .stats-row {\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  .card-header {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n  }\r\n  \r\n  .recent-books {\r\n    margin: 20px 0;\r\n  }\r\n  \r\n  .todo-row {\r\n    margin-top: 20px;\r\n  }\r\n  </style>"],"mappings":"AA8CE,SAASA,QAAO,QAAS,MAAK;AAC9B,OAAOC,QAAO,MAAO,gCAA+B;AACpD,OAAOC,YAAW,MAAO,oCAAmC;AAC5D,OAAOC,QAAO,MAAO,gCAA+B;AACpD,OAAOC,cAAa,MAAO,sCAAqC;AAEhE,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVL,QAAQ;IACRC,YAAY;IACZC,QAAQ;IACRC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CACL;QACEC,KAAK,EAAE,MAAM;QACbC,KAAK,EAAE,IAAI;QACXC,IAAI,EAAE,oBAAoB;QAC1BC,KAAK,EAAE;MACT,CAAC,EACD;QACEH,KAAK,EAAE,MAAM;QACbC,KAAK,EAAE,KAAK;QACZC,IAAI,EAAE,cAAc;QACpBC,KAAK,EAAE;MACT,CAAC,EACD;QACEH,KAAK,EAAE,MAAM;QACbC,KAAK,EAAE,KAAK;QACZC,IAAI,EAAE,yBAAyB;QAC/BC,KAAK,EAAE;MACT,CAAC,EACD;QACEH,KAAK,EAAE,MAAM;QACbC,KAAK,EAAE,IAAI;QACXC,IAAI,EAAE,qBAAqB;QAC3BC,KAAK,EAAE;MACT;IAEJ;EACF,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGb,QAAQ,CAAC,OAAO,EAAE,CAAC,aAAa,CAAC;EACtC,CAAC;EACD,MAAMc,OAAOA,CAAA,EAAG;IACd,MAAM,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,wBAAwB;EACrD;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}