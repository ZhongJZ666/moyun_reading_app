{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapState, mapGetters, mapActions } from 'vuex';\nimport BookCard from '@/components/BookCard.vue';\nexport default {\n  name: 'BookListView',\n  components: {\n    BookCard\n  },\n  data() {\n    return {\n      searchQuery: '',\n      page: 1,\n      pageSize: 12\n    };\n  },\n  computed: {\n    ...mapState('books', ['books', 'loading', 'error']),\n    ...mapGetters('auth', ['isMentor']),\n    ...mapGetters('auth', ['isStudent']),\n    total() {\n      return this.books?.length || 0;\n    }\n  },\n  async created() {\n    try {\n      await this.fetchBooks();\n    } catch (error) {\n      console.error('获取书籍列表失败:', error);\n    }\n  },\n  methods: {\n    ...mapActions('books', ['fetchBooks']),\n    goToDetail(bookId) {\n      if (bookId) {\n        this.$router.push(`/books/${bookId}`);\n      }\n    },\n    goToRecommend() {\n      this.$router.push('/recommend');\n    },\n    goToMyBooks() {\n      this.$router.push('/my-books');\n    },\n    async addToMyBooks(book) {\n      // 这里调用API添加到我的书库，后续补充\n      this.$message.success(`已添加《${book.title}》到我的书库`);\n    },\n    async handleSearch() {\n      this.page = 1;\n      try {\n        await this.fetchBooks({\n          query: this.searchQuery,\n          page: this.page,\n          pageSize: this.pageSize\n        });\n      } catch (error) {\n        console.error('搜索书籍失败:', error);\n      }\n    },\n    async handlePageChange(page) {\n      this.page = page;\n      try {\n        await this.fetchBooks({\n          query: this.searchQuery,\n          page: this.page,\n          pageSize: this.pageSize\n        });\n      } catch (error) {\n        console.error('切换页面失败:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["mapState","mapGetters","mapActions","BookCard","name","components","data","searchQuery","page","pageSize","computed","total","books","length","created","fetchBooks","error","console","methods","goToDetail","bookId","$router","push","goToRecommend","goToMyBooks","addToMyBooks","book","$message","success","title","handleSearch","query","handlePageChange"],"sources":["E:\\moyun-reading-platform\\frontend\\src\\views\\books\\BookList.vue"],"sourcesContent":["<template>\r\n    <div class=\"book-list-container\">\r\n      <div class=\"header\">\r\n        <h2>书库</h2>\r\n        <el-button v-if=\"isMentor || isStudent\" type=\"primary\" @click=\"goToRecommend\">推荐书籍</el-button>\r\n        <el-button v-if=\"isMentor || isStudent\" type=\"success\" @click=\"goToMyBooks\">我的书库</el-button>\r\n      </div>\r\n      \r\n      <el-divider />\r\n      \r\n      <div class=\"filter-bar\">\r\n        <el-input\r\n          v-model=\"searchQuery\"\r\n          placeholder=\"搜索书名或作者\"\r\n          clearable\r\n          style=\"width: 300px\"\r\n          @clear=\"handleSearch\"\r\n          @keyup.enter=\"handleSearch\"\r\n        >\r\n          <template #append>\r\n            <el-button icon=\"el-icon-search\" @click=\"handleSearch\" />\r\n          </template>\r\n        </el-input>\r\n      </div>\r\n  \r\n      <div v-loading=\"loading\" class=\"book-grid\">\r\n        <template v-if=\"error\">\r\n          <el-alert\r\n            :title=\"error\"\r\n            type=\"error\"\r\n            show-icon\r\n            :closable=\"false\"\r\n          />\r\n        </template>\r\n        <template v-else-if=\"books && books.length > 0\">\r\n          <book-card\r\n            v-for=\"book in books\"\r\n            :key=\"book.id\"\r\n            :book=\"book\"\r\n            @click.native=\"goToDetail(book.id)\"\r\n          >\r\n            <template #actions>\r\n              <el-button v-if=\"isMentor || isStudent\" size=\"mini\" type=\"primary\" @click.stop=\"addToMyBooks(book)\">加入我的书库</el-button>\r\n            </template>\r\n          </book-card>\r\n        </template>\r\n        <template v-else>\r\n          <el-empty description=\"暂无书籍\" />\r\n        </template>\r\n      </div>\r\n  \r\n      <el-pagination\r\n        v-if=\"total > 0\"\r\n        layout=\"prev, pager, next\"\r\n        :total=\"total\"\r\n        :page-size=\"pageSize\"\r\n        @current-change=\"handlePageChange\"\r\n      />\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import { mapState, mapGetters, mapActions } from 'vuex'\r\n  import BookCard from '@/components/BookCard.vue'\r\n  \r\n  export default {\r\n    name: 'BookListView',\r\n    components: { BookCard },\r\n    data() {\r\n      return {\r\n        searchQuery: '',\r\n        page: 1,\r\n        pageSize: 12\r\n      }\r\n    },\r\n    computed: {\r\n      ...mapState('books', ['books', 'loading', 'error']),\r\n      ...mapGetters('auth', ['isMentor']),\r\n      ...mapGetters('auth', ['isStudent']),\r\n      total() {\r\n        return this.books?.length || 0\r\n      }\r\n    },\r\n    async created() {\r\n      try {\r\n        await this.fetchBooks()\r\n      } catch (error) {\r\n        console.error('获取书籍列表失败:', error)\r\n      }\r\n    },\r\n    methods: {\r\n      ...mapActions('books', ['fetchBooks']),\r\n      goToDetail(bookId) {\r\n        if (bookId) {\r\n          this.$router.push(`/books/${bookId}`)\r\n        }\r\n      },\r\n      goToRecommend() {\r\n        this.$router.push('/recommend')\r\n      },\r\n      goToMyBooks() {\r\n        this.$router.push('/my-books')\r\n      },\r\n      async addToMyBooks(book) {\r\n        // 这里调用API添加到我的书库，后续补充\r\n        this.$message.success(`已添加《${book.title}》到我的书库`)\r\n      },\r\n      async handleSearch() {\r\n        this.page = 1\r\n        try {\r\n          await this.fetchBooks({\r\n            query: this.searchQuery,\r\n            page: this.page,\r\n            pageSize: this.pageSize\r\n          })\r\n        } catch (error) {\r\n          console.error('搜索书籍失败:', error)\r\n        }\r\n      },\r\n      async handlePageChange(page) {\r\n        this.page = page\r\n        try {\r\n          await this.fetchBooks({\r\n            query: this.searchQuery,\r\n            page: this.page,\r\n            pageSize: this.pageSize\r\n          })\r\n        } catch (error) {\r\n          console.error('切换页面失败:', error)\r\n        }\r\n      }\r\n    }\r\n  }\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .book-list-container {\r\n    padding: 20px;\r\n    max-width: 1200px;\r\n    margin: 0 auto;\r\n  }\r\n  \r\n  .header {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n  }\r\n  \r\n  .filter-bar {\r\n    margin: 20px 0;\r\n  }\r\n  \r\n  .book-grid {\r\n    display: grid;\r\n    grid-template-columns: repeat(auto-fill, minmax(200px, 1fr));\r\n    gap: 20px;\r\n    margin: 20px 0;\r\n  }\r\n  \r\n  .el-pagination {\r\n    margin-top: 20px;\r\n    text-align: center;\r\n  }\r\n  </style>"],"mappings":";AA8DE,SAASA,QAAQ,EAAEC,UAAU,EAAEC,UAAS,QAAS,MAAK;AACtD,OAAOC,QAAO,MAAO,2BAA0B;AAE/C,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAE;IAAEF;EAAS,CAAC;EACxBG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,IAAI,EAAE,CAAC;MACPC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGV,QAAQ,CAAC,OAAO,EAAE,CAAC,OAAO,EAAE,SAAS,EAAE,OAAO,CAAC,CAAC;IACnD,GAAGC,UAAU,CAAC,MAAM,EAAE,CAAC,UAAU,CAAC,CAAC;IACnC,GAAGA,UAAU,CAAC,MAAM,EAAE,CAAC,WAAW,CAAC,CAAC;IACpCU,KAAKA,CAAA,EAAG;MACN,OAAO,IAAI,CAACC,KAAK,EAAEC,MAAK,IAAK;IAC/B;EACF,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAM,IAAI,CAACC,UAAU,CAAC;IACxB,EAAE,OAAOC,KAAK,EAAE;MACdC,OAAO,CAACD,KAAK,CAAC,WAAW,EAAEA,KAAK;IAClC;EACF,CAAC;EACDE,OAAO,EAAE;IACP,GAAGhB,UAAU,CAAC,OAAO,EAAE,CAAC,YAAY,CAAC,CAAC;IACtCiB,UAAUA,CAACC,MAAM,EAAE;MACjB,IAAIA,MAAM,EAAE;QACV,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,UAAUF,MAAM,EAAE;MACtC;IACF,CAAC;IACDG,aAAaA,CAAA,EAAG;MACd,IAAI,CAACF,OAAO,CAACC,IAAI,CAAC,YAAY;IAChC,CAAC;IACDE,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACH,OAAO,CAACC,IAAI,CAAC,WAAW;IAC/B,CAAC;IACD,MAAMG,YAAYA,CAACC,IAAI,EAAE;MACvB;MACA,IAAI,CAACC,QAAQ,CAACC,OAAO,CAAC,OAAOF,IAAI,CAACG,KAAK,QAAQ;IACjD,CAAC;IACD,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI