 { required: true, message: '请输入邮箱地址', trigger: 'blur' },\r\n          { type: 'email', message: '请输入正确的邮箱地址', trigger: 'blur' }\r\n        ],\r\n        role: [\r\n          { required: true, message: '请选择角色', trigger: 'change' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  watch: {\r\n    visible(val) {\r\n      this.dialogVisible = val\r\n      if (val && this.userData) {\r\n        this.form = { ...this.userData }\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    handleClose() {\r\n      this.$emit('update:visible', false)\r\n      this.$refs.formRef?.resetFields()\r\n    },\r\n    handleSubmit() {\r\n      this.$refs.formRef.validate((valid) => {\r\n        if (valid) {\r\n          this.$emit('submit', this.form)\r\n          this.dialogVisible = false\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.dialog-footer {\r\n  display: flex;\r\n  justify-content: flex-end;\r\n  gap: 10px;\r\n}\r\n</style> "],"mappings":"AAwCA,eAAe;EACbA,IAAI,EAAE,gBAAgB;EACtBC,KAAK,EAAE;IACLC,OAAO,EAAE;MACPC,IAAI,EAAEC,OAAO;MACbC,OAAO,EAAE;IACX,CAAC;IACDC,QAAQ,EAAE;MACRH,IAAI,EAAEI,MAAM;MACZF,OAAO,EAAEA,CAAA,MAAO,CAAC,CAAC;IACpB;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,KAAK;MACpBC,IAAI,EAAE;QACJC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,IAAI,EAAE,MAAM;QACZC,MAAM,EAAE;MACV,CAAC;MACDC,KAAK,EAAE;QACLJ,QAAQ,EAAE,CACR;UAAEK,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,QAAQ;UAAEC,OAAO,EAAE;QAAO,CAAC,EACtD;UAAEC,GAAG,EAAE,CAAC;UAAEC,GAAG,EAAE,EAAE;UAAEH,OAAO,EAAE,gBAAgB;UAAEC,OAAO,EAAE;QAAO,EAC/D;QACDN,KAAK,EAAE,CACL;UAAEI,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,SAAS;UAAEC,OAAO,EAAE;QAAO,CAAC,EACvD;UAAEf,IAAI,EAAE,OAAO;UAAEc,OAAO,EAAE,YAAY;UAAEC,OAAO,EAAE;QAAO,EACzD;QACDL,IAAI,EAAE,CACJ;UAAEG,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,OAAO;UAAEC,OAAO,EAAE;QAAS;MAE1D;IACF;EACF,CAAC;EACDG,KAAK,EAAE;IACLnB,OAAOA,CAACoB,GAAG,EAAE;MACX,IAAI,CAACb,aAAY,GAAIa,GAAE;MACvB,IAAIA,GAAE,IAAK,IAAI,CAAChB,QAAQ,EAAE;QACxB,IAAI,CAACI,IAAG,GAAI;UAAE,GAAG,IAAI,CAACJ;QAAS;MACjC;IACF;EACF,